<h1>Listing encuesta</h1>

<table>
  <thead>
    <tr>
      <th>Edad</th>
      <th>Sexo</th>
      <th>Carrera</th>
      <th>Control robot</th>
      <th>Control neuro</th>
      <th>P3</th>
      <th>P4</th>
      <th>P5</th>
      <th>P6</th>
      <th>P7</th>
      <th>P8</th>
      <th>P9</th>
      <th>P10</th>
      <th>P11</th>
      <th>P12</th>
      <th>Exp1</th>
      <th>Exp2</th>
      <th>Exp3</th>
      <th>Exp4</th>
      <th>Exp5</th>
      <th>Exp6</th>
      <th>Exp7</th>
      <th>Exp8</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @encuesta.each do |encuestum| %>
      <tr>
        <td><%= encuestum.edad %></td>
        <td><%= encuestum.sexo %></td>
        <td><%= encuestum.carrera %></td>
        <td><%= encuestum.control_robot %></td>
        <td><%= encuestum.control_neuro %></td>
        <td><%= encuestum.p3 %></td>
        <td><%= encuestum.p4 %></td>
        <td><%= encuestum.p5 %></td>
        <td><%= encuestum.p6 %></td>
        <td><%= encuestum.p7 %></td>
        <td><%= encuestum.p8 %></td>
        <td><%= encuestum.p9 %></td>
        <td><%= encuestum.p10 %></td>
        <td><%= encuestum.p11 %></td>
        <td><%= encuestum.p12 %></td>
        <td><%= encuestum.exp1 %></td>
        <td><%= encuestum.exp2 %></td>
        <td><%= encuestum.exp3 %></td>
        <td><%= encuestum.exp4 %></td>
        <td><%= encuestum.exp5 %></td>
        <td><%= encuestum.exp6 %></td>
        <td><%= encuestum.exp7 %></td>
        <td><%= encuestum.exp8 %></td>
        <td><%= link_to 'Show', encuestum %></td>
        <td><%= link_to 'Edit', edit_encuestum_path(encuestum) %></td>
     
      </tr>
    <% end %>
  </tbody>
</table>

